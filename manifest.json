{
  "manifest_version": 2,
  "name": "Spree",
  "description":"Speed read web text without having to move your eyes.",
  "version":"1.0.0",
  "icons":{"16":"16.png","32":"32.png"},
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js":["spree.js"],
      "run_at":"document_start"
    }
  ]
}
