{
  "manifest_version": 2,
  "name": "Spree",
  "description":"Speed read web text without having to move your eyes.",
  "version":"1.4.1",
  "icons":{"16":"16.png","48":"48.png","128":"128.png"},
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js":["spree.min.js"],
      "run_at":"document_start"
    }
  ]
}
